{
  "name": "@folkdb/abc-render-svg",
  "version": "1.0.0",
  "description": "Render ABC music notation to SVG in a Node environment",
  "author": "Daniel C. Narey <os@metamodern.design>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/folkdb/abc-render-svg"
  },
  "type": "module",
  "module": "./lib/index.js",
  "exports": {
    ".": "./lib/index.js"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "lint": "eslint --fix lib/*.js",
    "release": "np",
    "sync": "git pull && git add . && git commit -m 'automated sync' && git push",
    "test": "ava test/*.js",
    "update": "updtr"
  },
  "dependencies": {
    "jsdom": "^16.3.0"
  },
  "devDependencies": {
    "abcjs": "^5.12.0",
    "ava": "^3.10.1",
    "eslint": "^7.5.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-ava": "^10.4.0",
    "eslint-plugin-import": "^2.20.2",
    "np": "^6.3.2",
    "updtr": "^3.1.0"
  },
  "engines": {
    "node": ">=12"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "abc",
    "abcjs",
    "music",
    "notation",
    "node",
    "render",
    "svg",
    "backend",
    "server"
  ],
  "ava": {
    "nodeArguments": [
      "--experimental-modules"
    ]
  }
}
